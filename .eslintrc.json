{
  "extends": "airbnb",
  "rules": {
    "indent": [
      "error",
      2
    ],
    "react/jsx-indent": [
      "error",
      2
    ],
    "react/jsx-indent-props": [
      "error",
      2
    ],
    "react/prop-types": "off",
    "react/no-array-index-key": "off",
    "no-param-reassign": 0
  }
}